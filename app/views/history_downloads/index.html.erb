<h3 class="text-center">History Downloads</h3>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Number</th>
      <th>Name Document</th>
      <th>Time</th>
    </tr>
  </thead>
  <tbody>
    <% if @histories.any? %>
      <%= render partial: "history_download", collection: @histories, as: :history %>
    <% end %>
    <%= will_paginate @histories %>
  </tbody>
</table>
